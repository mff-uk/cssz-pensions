### APPLICABLE FOR 2012, 2011, 2010, 2009, 2008
def-dc-penze:vydaje-na-duchody-od-roku-1996 a qb:DataStructureDefinition ;
  rdfs:label "The definition of the structure of the data cube of the total amount of money spent on pensions the Czech Republic classified by years."@en, "Definice struktury datové kostky s celkovými výdaji na důchody v České republice v členění dle let."@cs ;
  # Dimensions
  qb:component [
    qb:dimension def-dc-penze:refPeriod ;
    qb:order 1 ;
    rdfs:label "The dimension representing a year in which the spendings on pensions were measured."@en, "Dimenze reprezentující rok, za který výdaje uvedeny."@cs
  ] ;
  qb:component [
    qb:dimension def-dc-penze:druh-duchodu ;
    qb:order 2 ;
    rdfs:label "The dimension representing a kind of pension for which the spendings on pensions was measured."@en, "Dimenze reprezentující druh důchodu, pro který jsou výděje na důchody uvedeny."@cs
  ] ;
  # Measure
  qb:component [
    qb:measure def-dc-penze:vydaje-na-duchody-opravene-o-zalohy-v-tis-kc ;
    rdfs:label "The measure of the spendings on pensions in thousands of CZK. Spendigs are adjusted for cash advances. Spedings in the given year include the cash advance from December of the previous year, however it does not include the cash advandce of December of the current year allocated for the pensions that will be paid in January of the coming year."@en,
               "Měření výdajů na důchody v tisících Kč. Výdaje jsou opraveny o zálohy. Do výdajů je zahrnuta záloha z prosince předcházejícího roku a není do nich zahrnuta záloha z prosince aktuálního roku určená na výplatu důchodů v roce následujícím."@cs
  ] ;
  # Attributes
  qb:component [
    qb:attribute sdmx-attribute:unitMeasure ;
    rdfs:label "Unit of the measure"@en, "Jednotka měření"@cs ;
    qb:componentRequired "true"^^xsd:boolean ;
    qb:componentAttachment qb:DataSet
  ] .

